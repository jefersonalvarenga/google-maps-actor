{
    "actorSpecification": 1,
    "name": "google-maps-scraper",
    "title": "Google Maps Scraper",
    "version": "1.0.0",
    "description": "Extrai dados de estabelecimentos do Google Maps usando lista de termos de busca em uma localização específica",
    "dockerfile": "./Dockerfile",
    "readme": "./README.md",
    "input": "./INPUT_SCHEMA.json",
    "defaultRunOptions": {
        "build": "latest",
        "timeoutSecs": 3600,
        "memoryMbytes": 4096
    },
    "storages": {
        "dataset": {
            "actorSpecification": 1,
            "title": "Google Maps Results",
            "description": "Resultados extraídos do Google Maps",
            "views": {
                "default": {
                    "title": "Todos os resultados",
                    "transformation": {
                        "fields": [
                            "name",
                            "place_id",
                            "cid",
                            "knowledge_graph_id",
                            "rating",
                            "reviews_count",
                            "rating_distribution",
                            "category_primary",
                            "categories",
                            "price_level",
                            "business_status",
                            "full_address",
                            "street",
                            "city",
                            "state",
                            "postal_code",
                            "country_code",
                            "latitude",
                            "longitude",
                            "phone",
                            "whatsapp",
                            "website",
                            "services",
                            "google_maps_url",
                            "search_term",
                            "location",
                            "scraped_at"
                        ]
                    }
                }
            }
        }
    }
}
