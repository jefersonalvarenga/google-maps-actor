{
    "actorSpecification": 1,
    "name": "google-maps-scraper",
    "title": "Google Maps Scraper",
    "version": "1.0.0",
    "description": "Extrai dados de estabelecimentos do Google Maps usando lista de termos de busca em uma localização específica",
    "dockerfile": "./Dockerfile",
    "readme": "./README.md",
    "input": "./INPUT_SCHEMA.json",
    "defaultRunOptions": {
        "build": "latest",
        "timeoutSecs": 3600,
        "memoryMbytes": 2048
    },
    "storages": {
        "dataset": {
            "actorSpecification": 1,
            "title": "Google Maps Results",
            "description": "Resultados extraídos do Google Maps",
            "views": {
                "default": {
                    "title": "Todos os resultados",
                    "transformation": {
                        "fields": [
                            "title",
                            "totalScore",
                            "reviewsCount",
                            "categoryName",
                            "street",
                            "city",
                            "state",
                            "postalCode",
                            "phone",
                            "website",
                            "url",
                            "searchTerm",
                            "location"
                        ]
                    }
                }
            }
        }
    }
}
